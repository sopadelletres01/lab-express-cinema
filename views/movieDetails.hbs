{{#with movie}}
<div>
    <img src="{{image}}" alt="">
    <a href="/movies">Go Back</a>
    <h1>{{title}}</h1>
    <h3>Director: {{director}}</h3>
    <h4>Stars</h4>
    <ul>
        {{#each stars}}
            <li>{{this}}</li>
        {{/each}}
    </ul>
    <p>{{description}}</p>
     {{#each showtimes}}
        <span>{{this}}|</span>
    {{/each}}
</div>
{{/with}}